programa {
  funcao inicio() {
    //Descrevendo vareaveis
    inteiro valor

    //coletando dados

    escreva("Digite um valor: ")
    leia(valor)

    //area de calculo
    se (valor > 10) {
      escreva("É maior que 10!")

    } senao se (valor < 10) {
      escreva("Não é maior que 10!")
    } 
    
    se (valor == 10) {
          escreva("Valor igual a 10!")
        }
    

    escreva("\n=== Fim do programa ===")


  }
}
